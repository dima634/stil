{
    "version": 8,
    "configurePresets": [
        {
            "name": "debug",
            "displayName": "Debug",
            "description": "Using compilers: C = /usr/bin/clang, CXX = /usr/bin/clang++",
            "binaryDir": "${sourceDir}/eleven/out/build/${presetName}",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/eleven/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/clang",
                "CMAKE_CXX_COMPILER": "/usr/bin/clang++",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_CXX_CLANG_TIDY": "clang-tidy",
                "CMAKE_CXX_FLAGS": "-Wall -Wextra -Werror",
                "STIL_CORE_LIB_DIR": "${sourceDir}/target/debug"
            }
        },
        {
            "name": "dev",
            "displayName": "Development",
            "description": "Using compilers: C = /usr/bin/clang, CXX = /usr/bin/clang++",
            "binaryDir": "${sourceDir}/eleven/out/build/${presetName}",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/eleven/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/clang",
                "CMAKE_CXX_COMPILER": "/usr/bin/clang++",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_CXX_FLAGS": "-Wall -Wextra -Werror",
                "STIL_CORE_LIB_DIR": "${sourceDir}/target/debug"
            }
        },
        {
            "name": "release",
            "displayName": "Release",
            "description": "Using compilers: C = /usr/bin/clang, CXX = /usr/bin/clang++",
            "binaryDir": "${sourceDir}/eleven/out/build/${presetName}",
            "generator": "Ninja",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/eleven/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/bin/clang",
                "CMAKE_CXX_COMPILER": "/usr/bin/clang++",
                "CMAKE_BUILD_TYPE": "Release",
                "CMAKE_CXX_CLANG_TIDY": "clang-tidy",
                "CMAKE_CXX_FLAGS": "-Wall -Wextra -Werror",
                "STIL_CORE_LIB_DIR": "${sourceDir}/target/release"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "debug",
            "displayName": "Plugin Debug Build",
            "configurePreset": "debug",
            "configuration": "Debug"
        },
        {
            "name": "dev",
            "displayName": "Plugin Development Build",
            "configurePreset": "dev",
            "configuration": "Debug"
        },
        {
            "name": "release",
            "displayName": "Plugin Release Build",
            "configurePreset": "release",
            "configuration": "Release"
        }
    ]
}